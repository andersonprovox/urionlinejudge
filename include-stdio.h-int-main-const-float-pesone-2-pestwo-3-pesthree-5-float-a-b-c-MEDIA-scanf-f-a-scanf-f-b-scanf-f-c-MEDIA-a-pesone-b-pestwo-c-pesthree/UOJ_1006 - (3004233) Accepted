#include <stdio.h>
 
int main() {
 
    const float pesone = 2, pestwo = 3, pesthree = 5;
       float a, b, c, MEDIA;

       scanf("%f", &a);
       scanf("%f", &b);
       scanf("%f", &c);

       MEDIA = (a * pesone + b * pestwo + c * pesthree) / (pesone + pestwo + pesthree);
       
       printf("MEDIA = %.1f\n", MEDIA);
 
    return 0;
}
